// This is your Prisma schema file

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}






model PipelineDummy {
  name      String   @id  // Primary key
  id        String   @unique  // Optional unique field
  runId     String?
  status    String?
  message   String?
  runStart  DateTime?
  runEnd    DateTime?
  properties Json?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("PipelineDummy")
}